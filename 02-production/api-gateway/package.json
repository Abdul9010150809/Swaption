{
  "name": "price-matrix-api-gateway",
  "version": "1.0.0",
  "description": "API Gateway for Price Matrix financial pricing system",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "build": "npm run lint && npm test",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js"
  },
  "keywords": [
    "finance",
    "pricing",
    "api",
    "gateway",
    "derivatives",
    "options",
    "swaptions"
  ],
  "author": "Price Matrix Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "http-proxy-middleware": "^2.0.6",
    "axios": "^1.4.0",
    "redis": "^4.6.7",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "prometheus-api-metrics": "^3.2.2",
    "response-time": "^2.3.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "eslint": "^8.45.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "pm2": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/price-matrix.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/price-matrix/issues"
  },
  "homepage": "https://github.com/your-org/price-matrix#readme",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}